{
    "preset": "jquery",
    "validateIndentation": 2,
    "validateQuoteMarks": "'",
    "requireCurlyBraces": [ "else", "for", "while", "do" ],
    "disallowMultipleVarDecl": null,
    "requireMultipleVarDecl": true,
    "esnext": true,
    "disallowSpaceAfterKeywords": [
      "if",
      "else",
      "for",
      "while",
      "do",
      "switch",
      "try",
      "catch"
    ]
}
